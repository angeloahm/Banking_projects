   "source": [
    "var_pre_2017 = ['RIADA517', 'RIADA518', 'RIAD4508', 'RIAD0093']\n",
    "var_post_2017 = ['RIADHK03', 'RIADHK04', 'RIAD4508', 'RIAD0093']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "metadata": {},
